{
	"name": "fexel",
	"version": "0.0.1",
	"devDependencies": {
		"assemblyscript": "AssemblyScript/assemblyscript"
	},
	"scripts": {
		"asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --importMemory --sourceMap --validate --debug",
		"asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --importMemory --sourceMap --validate --optimize",
		"asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
	}
}
